<Window x:Class="ZdravoCorp.ManagerView.ManagerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ZdravoCorp"
        mc:Ignorable="d"
        Title="ManagerWindow" Height="450" Width="800" Icon="/icon.png">
    <Grid>
        
        <Menu>
            <MenuItem Header="Filter Equipment" FontSize="16" Click="FilterClick">
            </MenuItem>
            <MenuItem Header="Order Dynamic Equipment" FontSize="16" Click="OrderClick">
            </MenuItem>
            <MenuItem Header="Transfer Static Equipment" FontSize="16" Click="StaticTransferClick">
            </MenuItem>
            <MenuItem Header="Transfer Dynamic Equipment" FontSize="16" Click="DynamicTransferClick">
            </MenuItem>
            <MenuItem Header="LogOut" FontSize="16" Click="LogOutClick"></MenuItem>
        </Menu>
    </Grid>
</Window>
